<main class="ec-cor-barometer">
  
  <!-- Banner -->
  <section class="ec-cor-barometer-banner">
    {% render '@banner' %}
  </section>
  <!-- /Banner -->

  <!-- Web streaming section -->
  <section class="ec-cor-barometer-webstreaming container">
    <h2 class="ec-cor-barometer-webstreaming__title ec-cor-heading--2">{{ video.title }}</h2>
    {% render '@embed' %}
    <!--p>
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
      <small>also available in <a href="">other languages</a></small>
    </p-->
    
    <div class="ec-cor-barometer-webstreaming__links">
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
      <small>also available in <a href="">FR</a> / <a href="">DE</a> / <a href="">GR</a></small>
    </div>
    
    <!--p>
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
      <a class="ec-cor-barometer-link" href="#"><span>Read the full speech</span></a>
    </p-->
    {% render '@quote' %}
  </section>
  <!-- /Web streaming section -->

  <div class="ec-cor-barometer-separator"></div>

