{% macro programItem(speach) %}
  

  <div class="ec-europcom-program__item">

    

    <div class="ec-europcom-program__item__time">{{ speach.time }}</div>

    <div>
      {% if speach.workshops %}
      <strong>{{ speach.title | safe}}</strong>
      <div class="ec-europcom-program__item__workshops">
          
        
          
            {% for workshop in speach.workshops %}
            <div class="ec-europcom-program__item__workshops__details-container">
              <div class="ec-europcom-program__item__img ec-europcom-program__item__img--{{ workshop.category | lower }} mt-1">
                <img src="/images/{{ workshop.category | lower }}.svg" alt="{{ workshop.category | lower }} theme logo"/>
              </div>
              
              <div>
                <small class="text-uppercase">
                  {{workshop.category }}
                </small>
                <details>
                  <summary>
                    {{ workshop.title }} 
                  </summary>
                  <div class="mt-1 p-3 ec-europcom-program__item__workshops__details">{{ workshop.more }}</div>
                </details>
              </div>
            </div>
            {% endfor %}
      
          
        
      
      </div>
      {% else %}
        <details>
          <summary>{{ speach.title | safe }}</summary>

          <div class="ec-europcom-program__item__description my-3 p-3">
            {{ speach.description }}

            <div id="Speakers-2"><h4 class="mt-3">Moderator</h4><ul class="ec-europcom-program__speakers-list ec-europcom-program__speakers-list--moderators"><li class="ec-europcom-program__speakers-list__item"><a href="/EuropCom/Pages/Contributors.aspx#15" target="_blank"><img class="rounded-circle img-fluid" alt="Méabh Mc Mahon portrait" src="https://cor.europa.eu/europcom/PublishingImages/Speakers/MOD%20-%20Méabh%20Mc%20Mahon%20-%20fin.jpg"><div class="ec-europcom-program__speakers-list__item__description text-center"><strong>Méabh Mc Mahon</strong><div>Reporter and news anchor, Euronews</div></div></a></li></ul><h4 class="mt-3">Speakers</h4><ul class="ec-europcom-program__speakers-list"><li class="ec-europcom-program__speakers-list__item"><a href="/EuropCom/Pages/Contributors.aspx#6" target="_blank"><img class="rounded-circle img-fluid" alt="Aura Salla portrait" src="https://cor.europa.eu/europcom/PublishingImages/Speakers/02%20-%20Aura%20Salla%20-%20fin.jpg"><div class="ec-europcom-program__speakers-list__item__description text-center"><strong>Aura Salla</strong><div>Head of EU Affairs, Facebook</div></div></a></li><li class="ec-europcom-program__speakers-list__item"><a href="/EuropCom/Pages/Contributors.aspx#16" target="_blank"><img class="rounded-circle img-fluid" alt="Michael Nathan portrait" src="https://cor.europa.eu/europcom/PublishingImages/Speakers/02%20-%20Michael%20Nathan%20-%20fin.jpg"><div class="ec-europcom-program__speakers-list__item__description text-center"><strong>Michael Nathan</strong><div>Managing Director, French Government Information Service</div></div></a></li><li class="ec-europcom-program__speakers-list__item"><a href="/EuropCom/Pages/Contributors.aspx#23" target="_blank"><img class="rounded-circle img-fluid" alt="Tom Brookes portrait" src="https://cor.europa.eu/europcom/PublishingImages/Speakers/02%20-%20Tom%20Brookes%20-%20fin.jpg"><div class="ec-europcom-program__speakers-list__item__description text-center"><strong>Tom Brookes</strong><div>Executive Director, Strategic Communications, European Climate Foundation</div></div></a></li></ul></div>
          </div>

        </details>
      {% endif %}
    </div>
    

        
  </div>
{% endmacro %}


<div class="ec-europcom-program">
  
  
<div class="btn-toolbar mt-5 mb-5">
  <div class="btn-group mr-2">
    <button id="summarydAgenda" type="button" class="btn btn-sm btn-outline-secondary active ec-europcom-program__change-detail-btn">Summary program</button>
    <button id="detailedAgenda" type="button" class="btn btn-sm btn-outline-secondary ec-europcom-program__change-detail-btn">Detailed program</button>
  </div>

  <div class="action-buttons">
    <button class="btn btn-outline" id="PrintButton" title="Print"> 
      Print 
      <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-printer-fill ml-1" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
        <path d="M5 1a2 2 0 0 0-2 2v1h10V3a2 2 0 0 0-2-2H5z"></path>
        <path fill-rule="evenodd" d="M11 9H5a1 1 0 0 0-1 1v3a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1v-3a1 1 0 0 0-1-1z"></path>
        <path fill-rule="evenodd" d="M0 7a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v3a2 2 0 0 1-2 2h-1v-2a2 2 0 0 0-2-2H5a2 2 0 0 0-2 2v2H2a2 2 0 0 1-2-2V7zm2.5 1a.5.5 0 1 0 0-1 .5.5 0 0 0 0 1z"></path>
      </svg>
    </button> <button class="btn btn-outline" id="DownloadPDF" title="Download PDF"> 
      Download 
      <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-file-earmark-arrow-down-fill ml-1" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd" d="M2 2a2 2 0 0 1 2-2h5.293A1 1 0 0 1 10 .293L13.707 4a1 1 0 0 1 .293.707V14a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V2zm7.5 1.5v-2l3 3h-2a1 1 0 0 1-1-1zm-1 4a.5.5 0 0 0-1 0v3.793l-1.146-1.147a.5.5 0 0 0-.708.708l2 2a.5.5 0 0 0 .708 0l2-2a.5.5 0 0 0-.708-.708L8.5 11.293V7.5z"/>
      </svg>
    </button>
  </div>
</div>
  
  <ul class="ec-europcom-program__tabs nav nav-tabs" id="program" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" href="#firstday" id="firstday-tab" data-toggle="tab" role="tab" aria-controls="firstday" aria-selected="true">Monday 7 December 2020</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" href="#secondday" id="secondday-tab" data-toggle="tab" role="tab" aria-controls="secondday" aria-selected="false">Thursday 8 December 2020</a>
    </li>
  </ul>
    
  <div class="Summit2019_Prog_Main tab-content" id="programContent">

      <div class="tab-pane fade show active" id="firstday" role="tabpanel" aria-labelledby="firstday-tab">
        <div class="groupingSession mt-2">
          <div class="col-12 Summit2019_Session_Date">Monday 7 December 2020</div>
        </div>
      
        {% for speach in program7th %}
          {{ programItem(speach) }}
        {% endfor %}
      </div>
      
      <div class="tab-pane fade" id="secondday" role="tabpanel" aria-labelledby="secondday-tab">
        <div class="groupingSession mt-2">
          <div class="col-12 Summit2019_Session_Date">Thursday 8 December 2020</div>
        </div>
      
        {% for speach in program8th %}
          {{ programItem(speach) }}
        {% endfor %}
      </div>


        
        

        
        

  </div>
</div>
